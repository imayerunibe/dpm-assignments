labs(title = paste("Prediction of IAT Scores by Neuroticism"), x = "Neuroticism", y = "IAT Score") +
theme_minimal() +
theme(legend.position = "none", plot.title = element_text(hjust = 0.5))
scatter_plot_neuroticism
